<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>SimpleXPath</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>XPathServlet</display-name>
    <servlet-name>XPathServlet</servlet-name>
    <servlet-class>com.bssys.service.XPathServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>XPathServlet</servlet-name>
    <url-pattern>/XPathServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>NameSpaceServlet</display-name>
    <servlet-name>NameSpaceServlet</servlet-name>
    <servlet-class>com.bssys.service.NameSpaceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NameSpaceServlet</servlet-name>
    <url-pattern>/NameSpaceServlet</url-pattern>
  </servlet-mapping>
</web-app>